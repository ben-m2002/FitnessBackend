{
  "family": "bylt-api",
  "networkMode": "bridge",
  "executionRoleArn": "arn:aws:iam::692350947347:role/ecsTaskExecutionRole",
  "cpu": "1024",
  "memory": "3072",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "IMAGE_URI",
      "cpu": 0,
      "memoryReservation": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_USERNAME",
          "value": "bylt_admin"
        },
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://bylt-db.ctq260e8ux4v.us-east-1.rds.amazonaws.com:5432/FitnessDB"
        },
        {
          "name": "SPRING_DATASOURCE_PASSWORD",
          "value": "Chivantor1234!"
        },
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "prod"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/bylt",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "volumes": []
}